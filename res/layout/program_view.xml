<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:id="@+id/program_view_layout"
    >
	<TextView android:layout_width="fill_parent"
	style="@style/titleBar"
	android:id="@+id/program_title"
		android:layout_height="wrap_content" android:text="@string/program_view_title" />
	<ImageView android:id="@+id/program_image"
		android:layout_width="wrap_content" android:layout_height="wrap_content"></ImageView>
	<WebView android:id="@+id/program_explan" android:layout_width="fill_parent"
		android:layout_height="fill_parent" android:layout_weight="1"></WebView>
	<TextView android:id="@+id/program_detail"
		android:layout_width="fill_parent" android:layout_height="wrap_content" />

	<Button android:text="접수목록보기" android:id="@+id/goReceiptList" style="@style/buttonStyle"
		android:layout_width="wrap_content" android:layout_height="wrap_content"></Button>
	
</LinearLayout>
