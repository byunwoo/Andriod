<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:id="@+id/organ_view_layout"
	android:orientation="vertical" android:layout_width="fill_parent"
	android:layout_height="fill_parent">
	<TextView android:layout_width="fill_parent"
		style="@style/titleBar"
		 android:id="@+id/organ_view_title"
		android:layout_height="wrap_content" android:text="@string/orgran_view_title" />
		<!-- 
	<TextView android:id="@+id/TextView01" android:layout_width="fill_parent"
		android:layout_height="wrap_content"></TextView>
		 -->
	<ImageView android:id="@+id/ImageView01"
		android:layout_width="fill_parent" android:layout_height="wrap_content"></ImageView>

	<WebView android:id="@+id/WebView01" android:layout_width="fill_parent"
		android:layout_height="fill_parent" android:layout_weight="1"></WebView>
	<TextView android:layout_width="fill_parent" android:id="@+id/view_debug"
		android:layout_height="wrap_content" />
	<Button android:text="하위프로그램보기" android:id="@+id/getProgram" style="@style/buttonStyle"
		android:layout_width="wrap_content" android:layout_height="wrap_content"></Button>
</LinearLayout>
